	<script type="text/javascript">
	window.bnConf.unGated = true
	window.bnConf.external.versaTag = { id: false }

	if ('https://bethesda.net' !== '') {
		window.bnConf.coreModel = 'https://bethesda.net/data/core'
	}

	bethnet(window.bnConf).then(function(api) {
		riot.mount('app', api)
		riot.mount('globalfooter', api)

		var initialState = api.store.getState()
		var initialLang = initialState.auth ? initialState.auth.lang : false
    var nodeBBLoggingIn = false
    var nodeBBLoggingOut = false
    // setting up pop up modal for use in nodebb
    window.showBethnetLogin = function(e) {
      if (e) {
        e.preventDefault()
      }
      api.showLogin()
    }

    // if there is a query string of cogs_modal=login then show the login modal
    if (/[?&]cogs_modal=login(&|$)/.test(window.location.search) && !config.loggedIn) {
      window.showBethnetLogin()
    }

		// if there is a query param of attunement_sso then grab the value and attempt to log into bnet
    if (/[?&]attunement_sso=[\s\S]*(&|$)/.test(window.location.search) && !config.loggedIn) {
			var queries = window.location.href.split('?')[1].split('&')
			queries.forEach(function(query, index){
				var keyValue = query.split('=')
				if(keyValue[0] === 'attunement_sso') {
					// Remove attunement_sso query from the queries array
					queries.splice(index, 1)
					api.authWithSSO(keyValue[1])
				}
			})
			var updatedUrl = window.location.origin + window.location.pathname
			if(queries.length) {
				updatedUrl = updatedUrl + '?' + queries.join('&')
			}
			// Update URL with the attunement_sso query removed
			window.history.pushState({}, '', updatedUrl)
    }

		// Check for whether cogs_modal=login or attunement_sso=* query is present
		if (!/[?&](cogs_modal=login|attunement_sso=[\s\S]*)(&|$)/.test(window.location.search)) {
			// On the initial load of header.tpl, set flag in session storage to be used
			// to determine if a language filter should be applied by default.
			sessionStorage.setItem('applyDefaultFilter', 'true');
		} else {
			// If the query contains cogs_modal=login or attunement_sso=*, remove flag from session storage
			// to prevent defaul filter taking precedence over those queries.
			sessionStorage.removeItem('applyDefaultFilter');
		}

		api.store.subscribe(function() {
			var state = api.store.getState()
			try {
				var lang = api.localization.i18n.language;
				if(initialLang) {
					lang = initialLang
					initialLang = false
				}
				if (lang === 'en') {
					lang = 'en-US';
				} else if (lang === 'pt') {
					lang = 'pt-BR'
				}
				// only request a different language for the user when the user is logged in
				if (config.loggedIn && config.userLang !== lang) {
					socket.emit('plugins.bnet.updateLang', { value: lang }, function() {
						config.userLang = lang;
						// Set flag to add default language filter on refresh
						sessionStorage.setItem('applyDefaultFilter', 'true');
            $.getScript(config.relative_path + '/assets/vendor/jquery/timeago/locales/jquery.timeago.' + config.userLang + '.js', function() {
              ajaxify.refresh();
            });
					});
				}
        var idToken = ((state || {}).cogs || {}).idToken
        // if nodebb is not logged in, but state says user is logged in, re-login nodebb
        if (!nodeBBLoggingIn && !config.loggedIn && idToken) {
          nodeBBLoggingIn = true
          nodeBBLoggingOut = false
          var app = window.app || {}
          app.flags = app.flags || {}
          app.flags._sessionRefresh = true
          $.ajax({
            url: config.relative_path + '/api/plugins/session-sharing/jwt',
            type: 'GET',
            beforeSend: function(xhr) {
              xhr.setRequestHeader('Content-Type', 'application/json')
              xhr.setRequestHeader('Authorization', 'Bearer ' + idToken)
            }
          }).done(function(data) {
            // need to do a refresh
            config.loggedIn = true
            if (data.needsRefresh) {
              window.location.reload();
            }
          }).always(function() {
            nodeBBLoggingIn = false
          })
        }
        // logged into nodebb but not bnet
        // and not logged in as uid 1
        if (!nodeBBLoggingOut && config.loggedIn && !idToken && config.uid !== 1) {
          nodeBBLoggingOut = true
          $.ajax({
            url: config.relative_path + '/api/plugins/session-sharing/signout',
            type: 'GET',
            beforeSend: function(xhr) {
              xhr.setRequestHeader('Content-Type', 'application/json')
            }
          }).done(function() {
            // need to do a refresh
            config.loggedIn = false
            window.location.href=window.location.href
          }).always(function() {
            nodeBBLoggingOut = false
          })
        }
			} catch(e) {};
			try {
				var categoryFilters = state.model ? state.model.categoryFilters : null
				var country = state.geolocation
				if (categoryFilters && country) {
					$(window).trigger('action:bethnet.geolocation', {
						categoryFilters: categoryFilters,
						country: country
					})
				};
			} catch(e) {};
		});

		$(window).trigger('action:bethnet.ready');
  }).catch(function(err) {
    console.trace(err.stack)
  })
	</script>

